{
    "server": {
        "name": "github.com/Meha555/pulse Server",
        "host": "127.0.0.1",
        "port": 3333,
        "heartbeat_tick": 3,
        "conn_timeout": 60,
        "max_conn_count": 100,
        "max_msg_queue_size": 50,
        "max_packet_size": 4096,
        "max_worker_pool_size": 10,
        "request_pool_mode": true
    },
    "log": {
        "level": 0,
        "format": "[%t] [%c %l] [%f:%L:%g] %m",
        "max_log_size": 104857600
    }
}